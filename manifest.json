{
    "name": "Burstmarks",
    "version": "0.2",
    "description": "快速定位最近使用的、最近添加的书签。",
    "permissions": [
        "tabs", "chrome://favicon/", "bookmarks", "history"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "打开 Burstmarks",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "manifest_version": 2
}